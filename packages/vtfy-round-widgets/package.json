{
  "name": "@thenightex/vtfy-round-widgets",
  "version": "1.2.10",
  "description": "vuetify round widgets",
  "scripts": {
    "build": "npm run build:module && npm run build:browser",
    "build:module": "bili --name vtfy-round-widgets --format cjs,es --plugin vue",
    "build:browser": "bili --name vtfy-round-widgets --format umd,umd-min --module-name vtfyRoundWidgets --external vue,vuetify/lib --global.vue \"Vue\" --plugin vue --global.vuetify/lib \"Vue.options.components\"",
    "lint": "eslint --ext js --ext vue src",
    "lint:fix": "npm run lint -- --fix",
    "prepublish": "npm run build"
  },
  "main": "dist/vtfy-round-widgets.cjs.js",
  "module": "dist/vtfy-round-widgets.m.js",
  "browser": "dist/vtfy-round-widgets.js",
  "cdn": "dist/vtfy-round-widgets.min.js",
  "unpkg": "dist/vtfy-round-widgets.min.js",
  "homepage": "https://github.com/thenightex/vtfy-widgets",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thenightex/vtfy-widgets.git",
    "directory": "packages/vtfy-round-widgets"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "email": "s.hanke1316@gmail.com",
    "name": "Sascha Hanke"
  },
  "license": "MIT",
  "keywords": [
    "vuetify",
    "widget",
    "round"
  ],
  "devDependencies": {
    "@vue/compiler-sfc": "^3.0.0-beta.4",
    "babel-eslint": "^10.1.0",
    "bili": "^1.6.5",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-vue": "^6.2.2",
    "eslint-plugin-vuetify": "^1.0.0-beta.6",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-vue": "^5.1.6",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11",
    "vuetify": "^2.2.25"
  },
  "peerDependencies": {
    "vue": "*",
    "vuetify": "*"
  },
  "dependencies": {
    "moment": "^2.24.0"
  },
  "bugs": {
    "url": "https://github.com/thenightex/vtfy-widgets/issues"
  }
}
