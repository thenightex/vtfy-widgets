{
  "name": "@thenightex/date-picker-field",
  "version": "1.0.2",
  "description": "vuetify datepicker field",
  "scripts": {
    "build": "npm run build:module && npm run build:browser",
    "build:module": "bili --name date-picker-field --format cjs,es --plugin vue",
    "build:browser": "bili --name date-picker-field --format umd,umd-min --module-name datepickerField --external vue,vuetify/lib --global.vue \"Vue\" --plugin vue --global.vuetify/lib \"Vue.options.components\"",
    "lint": "eslint --ext js --ext vue src",
    "lint:fix": "npm run lint -- --fix",
    "prepublish": "npm run build"
  },
  "main": "dist/date-picker-field.cjs.js",
  "module": "dist/date-picker-field.m.js",
  "browser": "dist/date-picker-field.js",
  "cdn": "dist/date-picker-field.min.js",
  "unpkg": "dist/date-picker-field.min.js",
  "homepage": "https://github.com/thenightex/vtfy-widgets",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thenightex/vtfy-widgets.git",
    "directory": "packages/date-picker-field"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "email": "s.hanke1316@gmail.com",
    "name": "Sascha Hanke"
  },
  "license": "MIT",
  "keywords": [
    "vuetify",
    "datepicker"
  ],
  "devDependencies": {
    "bili": "^1.6.5",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-vue": "^4.2.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-vue": "5.1.1",
    "vue": "^2.5.13",
    "vue-template-compiler": "^2.5.13",
    "vuetify": "^2.0.2"
  },
  "peerDependencies": {
    "vue": "*",
    "vuetify": "*"
  },
  "dependencies": {
    "moment": "^2.24.0"
  },
  "bugs": {
    "url": "https://github.com/thenightex/vtfy-widgets/issues"
  }
}
